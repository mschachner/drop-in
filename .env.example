# MongoDB connection string
MONGO_URL=mongodb://localhost:27017/dropin

# Server port (default: 3001)
PORT=3001

# Node environment
NODE_ENV=development

# Comma-separated list of allowed CORS origins (optional, has sensible defaults)
# CORS_ORIGINS=https://drop-in.up.railway.app,https://www.drop-in-cal.com,http://localhost:3000

# SHA-256 hash of the admin password (generate with: echo -n "yourpassword" | shasum -a 256)
ADMIN_PASSWORD_HASH=
